{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2020",
    "module": "es2020",
    "typeRoots": [
      "/node_modules/@types"
    ],
    "lib": [
      "es2020",
      "dom"
    ],
    "paths": {
      "ngx-restangular/*": [
        "projects/ngx-restangular/*",
        "dist/ngx-restangular/*"
      ],
      "ngx-restangular": [
        "projects/ngx-restangular",
        "dist/ngx-restangular"
      ]
    },
    "useDefineForClassFields": false
  },
}
